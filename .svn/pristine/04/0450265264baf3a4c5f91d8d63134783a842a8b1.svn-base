package jp.ikisaki.www;

public class SnsModel {

	private static int year = 0;
	private static int month = 0;
	private static int monthDay = 0;
	private static int hour = 0;
	private static int minute = 0;
	private static String placeName = "";
	private static String placeId = "";

	private static String word = "";
	private static int linkFlag = 0;
	private static String link = "";

	private static String eventName = "";

	public static String getEventName() {
		return eventName;
	}
	public static void setEventName(String eventName) {
		SnsModel.eventName = eventName;
	}
	public static int getYear() {
		return year;
	}
	public static void setYear(int year) {
		SnsModel.year = year;
	}
	public static int getMonth() {
		return month;
	}
	public static void setMonth(int month) {
		SnsModel.month = month;
	}
	public static int getMonthDay() {
		return monthDay;
	}
	public static void setMonthDay(int monthDay) {
		SnsModel.monthDay = monthDay;
	}
	public static int getHour() {
		return hour;
	}
	public static void setHour(int hour) {
		SnsModel.hour = hour;
	}
	public static int getMinute() {
		return minute;
	}
	public static void setMinute(int minute) {
		SnsModel.minute = minute;
	}
	public static String getPlaceName() {
		return placeName;
	}
	public static void setPlaceName(String place) {
		SnsModel.placeName = place;
	}
	public static String getWord() {
		return word;
	}
	public static void setWord(String word) {
		SnsModel.word = word;
	}
	public static int getLinkFlag() {
		return linkFlag;
	}
	public static void setLinkFlag(int linkFlag) {
		SnsModel.linkFlag = linkFlag;
	}
	public static String getLink() {
		return link;
	}
	public static void setLink(String link) {
		SnsModel.link = link;
	}
	public static String getPlaceId() {
		return placeId;
	}
	public static void setPlaceId(String placeId) {
		SnsModel.placeId = placeId;
	}


}
